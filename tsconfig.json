{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/ui/header.tsx", "src/components/ui/header.tsx", "src/app/page.jsx", "src/components/ui/main/carCatalog.jsx", "src/components/ui/dashboard/profile/profileCard.jsx", "src/app/catalog/page.jsx", "src/app/marketplace/page.jsx", "src/components/ui/catalog/searchBar.tsx", "src/app/(dashboard)/profile/page.tsx", "src/config/auth.js"],
  "exclude": ["node_modules"]
}
